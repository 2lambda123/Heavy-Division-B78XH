<ModelBehaviors>
    <!-- ## Overhead panel -->
    <Template Name="HEAVY_ELECTRICAL_Push_IFE_Template">
        <UseTemplate Name="HEAVY_ELECTRICAL_CIRCUIT_Push_Button_Airliner">
            <CIRCUIT>80</CIRCUIT>
            <SEQ1_NODE_ID>Decals_Text_131</SEQ1_NODE_ID>
            <SEQ2_NODE_ID>Decals_Text_129</SEQ2_NODE_ID>
        </UseTemplate>
    </Template>

    <Template Name="HEAVY_ELECTRICAL_Push_CABIN_Template">
        <UseTemplate Name="HEAVY_ELECTRICAL_CIRCUIT_Push_Button_Airliner">
            <CIRCUIT>81</CIRCUIT>
            <SEQ1_NODE_ID>Decals_Text_132</SEQ1_NODE_ID>
            <SEQ2_NODE_ID>Decals_Text_130</SEQ2_NODE_ID>
        </UseTemplate>
    </Template>

    <!-- Template for DRIVE buttons -->
    <Template Name="HEAVY_ELECTRICAL_Push_Drive_Template">
        <UseTemplate Name="HEAVY_ELECTRICAL_Drive_SubTemplate">
            <INTERACTION_TYPE>Push</INTERACTION_TYPE>
        </UseTemplate>
    </Template>

    <!-- SubTemplate for DRIVE buttons -->
    <!-- There is no implementation for actions (The cover is operable but the buttons are INOP right now) -->
    <Template Name="HEAVY_ELECTRICAL_Drive_SubTemplate">
        <Parameters Type="Default">
            <NODE_ID>ELECTRICAL_#INTERACTION_TYPE#_Drive_#ENGINE_ID#_#ID#</NODE_ID>
            <COVER_NODE_ID>ELECTRICAL_#INTERACTION_TYPE#_Drive_#ENGINE_ID#_#ID#_Cover</COVER_NODE_ID>

            <Switch Param="ENGINE_ID">
                <Case Value="1">
                    <Switch Param="ID">
                        <Case Value="1">
                            <ANIM_NAME>ELECTRICAL_#INTERACTION_TYPE#_Drive_1</ANIM_NAME>
                            <COVER_ANIM_NAME>ELECTRICAL_Push_Drive_1_Cover</COVER_ANIM_NAME>
                        </Case>
                        <Default>
                            <ANIM_NAME>ELECTRICAL_#INTERACTION_TYPE#_Drive_#ENGINE_ID#_#ID#</ANIM_NAME>
                            <COVER_ANIM_NAME>ELECTRICAL_#INTERACTION_TYPE#_Drive_#ENGINE_ID#_#ID#_Cover</COVER_ANIM_NAME>
                        </Default>
                    </Switch>
                </Case>
                <Case Value="2">
                    <Switch Param="ID">
                        <Case Value="2">
                            <COVER_ANIM_NAME>ELECTRICAL_Push_Drive_2_2</COVER_ANIM_NAME>
                        </Case>
                        <Default>
                            <ANIM_NAME>ELECTRICAL_#INTERACTION_TYPE#_Drive_#ENGINE_ID#_#ID#</ANIM_NAME>
                            <COVER_ANIM_NAME>ELECTRICAL_#INTERACTION_TYPE#_Drive_#ENGINE_ID#_#ID#_Cover</COVER_ANIM_NAME>
                        </Default>
                    </Switch>
                </Case>
                <Default>
                    <ANIM_NAME>ELECTRICAL_#INTERACTION_TYPE#_Drive_#ENGINE_ID#_#ID#</ANIM_NAME>
                    <COVER_ANIM_NAME>ELECTRICAL_#INTERACTION_TYPE#_Drive_#ENGINE_ID#_#ID#_Cover</COVER_ANIM_NAME>
                </Default>
            </Switch>
            <Switch Param="INTERACTION_TYPE">
                <Case Value="Push">
                    <Condition Check="TYPE">
                        <PUSH_TYPE>#TYPE#</PUSH_TYPE>
                    </Condition>
                    <WWISE_EVENT_1>fuel_pump_push_button_on</WWISE_EVENT_1>
                    <WWISE_EVENT_2>fuel_pump_push_button_off</WWISE_EVENT_2>
                    <NORMALIZED_TIME_1>0.1</NORMALIZED_TIME_1>
                    <NORMALIZED_TIME_2>0.5</NORMALIZED_TIME_2>
                    <Condition Check="POTENTIOMETER">
                        <True>
                            <POTENTIOMETER_SEQ1>#POTENTIOMETER#</POTENTIOMETER_SEQ1>
                            <POTENTIOMETER_SEQ2>#POTENTIOMETER#</POTENTIOMETER_SEQ2>
                        </True>
                    </Condition>
                    <Condition Check="SIMVAR_POWER_SETTING">
                        <True>
                            <SIMVAR_POWER_SETTING_SEQ1>#SIMVAR_POWER_SETTING#</SIMVAR_POWER_SETTING_SEQ1>
                            <SIMVAR_POWER_SETTING_SEQ2>#SIMVAR_POWER_SETTING#</SIMVAR_POWER_SETTING_SEQ2>
                        </True>
                    </Condition>
                </Case>
                <Case Value="Switch">
                    <Condition Check="TYPE">
                        <SWITCH_TYPE>#TYPE#</SWITCH_TYPE>
                    </Condition>
                    <WWISE_EVENT_1>fuel_valve_switch_on</WWISE_EVENT_1>
                    <WWISE_EVENT_2>fuel_valve_switch_off</WWISE_EVENT_2>
                </Case>
            </Switch>
        </Parameters>
        <Parameters Type="Default">
            <Switch Param="INTERACTION_TYPE">
                <Case Value="Push">
                    <SEQ1_EMISSIVE_CODE>1</SEQ1_EMISSIVE_CODE>
                    <SEQ2_EMISSIVE_CODE>0</SEQ2_EMISSIVE_CODE>
                </Case>
            </Switch>
            <TYPE>ON_OFF</TYPE>
        </Parameters>
        <Parameters Type="Override">
            <Switch Param="INTERACTION_TYPE">
                <Case Value="Push">
                    <TEMPLATE_TO_CALL>ASOBO_GT_Push_Button_Airliner</TEMPLATE_TO_CALL>
                    <DOWN_STATE_CODE>1</DOWN_STATE_CODE>
                </Case>
            </Switch>
            <TYPE>ON_OFF</TYPE>
        </Parameters>
        <Parameters Type="Override">
            <Condition Check="REVERSE_SEQ">
                <SEQ1_EMISSIVE_CODE>#SEQ2_EMISSIVE_CODE#</SEQ1_EMISSIVE_CODE>
                <SEQ2_EMISSIVE_CODE>#SEQ1_EMISSIVE_CODE#</SEQ2_EMISSIVE_CODE>
            </Condition>

            <Condition Valid="IS_INOP">
                <SEQ1_EMISSIVE_CODE>1</SEQ1_EMISSIVE_CODE>
                <SEQ2_EMISSIVE_CODE>0</SEQ2_EMISSIVE_CODE>
                <TOOLTIP_TITLE/>
                <TOOLTIP_TITLE_ID/>
                <TOOLTIPID>TT:COCKPIT.TOOLTIPS.INOP</TOOLTIPID>
                <Switch Param="INOP_POSITION">
                    <Case Value="UP">
                        <DOWN_STATE_CODE>0</DOWN_STATE_CODE>
                    </Case>
                    <Case Value="DOWN">
                        <DOWN_STATE_CODE>1</DOWN_STATE_CODE>
                    </Case>
                    <Default>
                        <DOWN_STATE_CODE>0</DOWN_STATE_CODE>
                    </Default>
                </Switch>
            </Condition>
        </Parameters>
        <Switch Param="INTERACTION_TYPE">
            <Case Value="Push">
                <Condition Check="TEMPLATE_TO_CALL" Match="ASOBO_GT_Push_Button_Airliner">
                    <True>
                        <UseTemplate Name="ASOBO_GT_Push_Button_Airliner">
                        </UseTemplate>
                    </True>
                </Condition>
            </Case>
            <Case Value="Switch">
                <Condition Check="COVER_NODE_ID">
                    <Component ID="#COVER_NODE_ID#" Node="#COVER_NODE_ID#">
                        <UseTemplate Name = "ASOBO_GT_Switch_Code">
                            <ANIMTIP_0>TT:COCKPIT.TOOLTIPS.GENERIC_BUTTON_GUARD_ON</ANIMTIP_0>
                            <ANIMTIP_1>TT:COCKPIT.TOOLTIPS.GENERIC_BUTTON_GUARD_OFF</ANIMTIP_1>
                            <LEFT_SINGLE_CODE>(O:XMLVAR_CoverUp) ! (&gt;O:XMLVAR_CoverUp)</LEFT_SINGLE_CODE>
                            <ANIM_CODE>(O:XMLVAR_CoverUp) 100 *</ANIM_CODE>
                            <ANIM_NAME>#COVER_ANIM_NAME#</ANIM_NAME>
                            <WWISE_EVENT_1>fuel_pump_cover_on</WWISE_EVENT_1>
                            <WWISE_EVENT_2>fuel_pump_cover_off</WWISE_EVENT_2>
                        </UseTemplate>
                    </Component>
                </Condition>
            </Case>
        </Switch>
    </Template>


