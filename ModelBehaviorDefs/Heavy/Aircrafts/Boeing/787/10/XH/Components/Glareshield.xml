    <Template Name="HEAVY_AUTOPILOT_Knob_Bank_Limit_Template">
        <Parameters Type="Defaults">
            <SETTINGS_COUNT>5</SETTINGS_COUNT>
            <NODE_ID>AUTOPILOT_Knob_Bank_Limit</NODE_ID>
            <ANIM_NAME>AUTOPILOT_Knob_Bank_Limit</ANIM_NAME>
            <WWISE_EVENT>autopilot_hdg_bank_knob</WWISE_EVENT>
            <WRAP>@HG_FALSE</WRAP>
            <USE_INPUT_EVENT_ID>AUTOPILOT</USE_INPUT_EVENT_ID>
            <SWITCH_DIRECTION>Horizontal</SWITCH_DIRECTION>
            <TT_ICON>ROTATE</TT_ICON>
        </Parameters>

        <Parameters Type="Override">
            <KNOB_ID>Bank_Limit</KNOB_ID>
            <NUM_STATES Process="Int">#SETTINGS_COUNT# 1 +</NUM_STATES>
            <CODE_POS_0>(&gt;B:AUTOPILOT_Bank_Limit_0)</CODE_POS_0>
            <CODE_POS_1>(&gt;B:AUTOPILOT_Bank_Limit_1)</CODE_POS_1>
            <CODE_POS_2>(&gt;B:AUTOPILOT_Bank_Limit_2)</CODE_POS_2>
            <CODE_POS_3>(&gt;B:AUTOPILOT_Bank_Limit_3)</CODE_POS_3>
            <CODE_POS_4>(&gt;B:AUTOPILOT_Bank_Limit_4)</CODE_POS_4>
            <CODE_POS_5>(&gt;B:AUTOPILOT_Bank_Limit_5)</CODE_POS_5>
            <STR_STATE_0>0</STR_STATE_0>
            <STR_STATE_1>1</STR_STATE_1>
            <STR_STATE_2>2</STR_STATE_2>
            <STR_STATE_3>3</STR_STATE_3>
            <STR_STATE_4>4</STR_STATE_4>
            <STR_STATE_5>5</STR_STATE_5>
            <SET_STATE_2>3 (&gt;K:AP_MAX_BANK_SET)</SET_STATE_2>
            <SET_STATE_3>2 (&gt;K:AP_MAX_BANK_SET)</SET_STATE_3>
            <SET_STATE_1>4 (&gt;K:AP_MAX_BANK_SET)</SET_STATE_1>
            <SET_STATE_4>1 (&gt;K:AP_MAX_BANK_SET)</SET_STATE_4>
            <SET_STATE_0>5 (&gt;K:AP_MAX_BANK_SET)</SET_STATE_0>
            <SET_STATE_5>0 (&gt;K:AP_MAX_BANK_SET)</SET_STATE_5>
            <STATE0_TEST>(B:AUTOPILOT_Bank_Limit, Number) 0 ==</STATE0_TEST>
            <STATE1_TEST>(B:AUTOPILOT_Bank_Limit, Number) 1 ==</STATE1_TEST>
            <STATE2_TEST>(B:AUTOPILOT_Bank_Limit, Number) 2 ==</STATE2_TEST>
            <STATE3_TEST>(B:AUTOPILOT_Bank_Limit, Number) 3 ==</STATE3_TEST>
            <STATE4_TEST>(B:AUTOPILOT_Bank_Limit, Number) 4 ==</STATE4_TEST>
            <STATE5_TEST>(B:AUTOPILOT_Bank_Limit, Number) 5 ==</STATE5_TEST>
            <OVERRIDE_ANIM_LENGTH Process="Int">600</OVERRIDE_ANIM_LENGTH>
        </Parameters>

        <Component ID="#NODE_ID#" Node="#NODE_ID#">
            <UseTemplate Name="HEAVY_AUTOPILOT_Knob_Template">
                <TOOLTIP_TITLE>@TT_Package.AUTOPILOT_KNOB_HDG_BANK_LIMIT_TITLE</TOOLTIP_TITLE>
                <TT_DESCRIPTION_ID>@TT_Package.AUTOPILOT_KNOB_HDG_BANK_LIMIT_ACTION</TT_DESCRIPTION_ID>
                <TT_VALUE>(A:AUTOPILOT MAX BANK ID, Number) #SETTINGS_COUNT# &lt; if{ (A:AUTOPILOT MAX BANK, degrees) '%d°' @sprintf } els{ @TT_Auto }</TT_VALUE>
                <GET_STATE_EXTERNAL>(A:AUTOPILOT MAX BANK ID, Number)</GET_STATE_EXTERNAL>
                <ENUM_VAL_TO_POS_EXTERNAL>-1 * #SETTINGS_COUNT# +</ENUM_VAL_TO_POS_EXTERNAL>
                <ANIM>EXTERNAL</ANIM>
            </UseTemplate>
        </Component>
    </Template>

