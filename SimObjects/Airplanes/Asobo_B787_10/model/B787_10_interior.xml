                <Component ID="APUStarter">
                    <!-- Name: APU Starter Knob -->
                    <!-- State: Requires TEST -->
                    <!-- Tester: ??? -->
                    <UseTemplate Name="ASOBO_ELECTRICAL_Switch_APU_Starter_Template">
                        <NODE_ID>ELECTRICAL_Knob_APU_Starter</NODE_ID>
                        <ANIM_NAME>ELECTRICAL_Knob_APU_Starter</ANIM_NAME>
                        <SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
                        <SWITCH_POSITION_VAR>XMLVAR_APU_StarterKnob_Pos</SWITCH_POSITION_VAR>
                        <ACTIVE_FUEL_PUMP_ON_RPM>7</ACTIVE_FUEL_PUMP_ON_RPM>
                    </UseTemplate>
                    <!-- End: APU Starter Knob -->

                    <!-- Name: APU Starter Annunciator -->
                    <!-- State: OK -->
                    <!-- Tester: Co-mi -->
                    <UseTemplate Name="ASOBO_GT_Push_Button_Airliner_Dummy">
                        <NODE_ID>ELECTRICAL_Indicator_APU</NODE_ID>
                        <SEQ1_EMISSIVE_CODE>
                            <!-- Store Previous APU Switch position to register -->
                            (O:PreviousAPUSwitchPosition) s1
                            <!-- Store APU Switch position to register -->
                            (L:XMLVAR_APU_StarterKnob_Pos) s2

                            <!-- if previous APU switch position == 1 (ON) and APU switch position == 2 (START) -->
                            l1 1 == l2 2 == and if{
                                <!-- Add 0.7 second to simulation time and store the value -->
                                (E:SIMULATION TIME, seconds) 0.7 + (&gt;O:FaultEmissiveTime)
                            }

                            <!-- Update Previous APU Switch position -->
                            l2 (&gt;O:PreviousAPUSwitchPosition)
                            <!-- Return TRUE if FAULT should be emissive -->
                            (E:SIMULATION TIME, seconds) (O:FaultEmissiveTime) &lt;
                        </SEQ1_EMISSIVE_CODE>
                        <NO_SEQ2>true</NO_SEQ2>
                    </UseTemplate>
                    <!-- End: APU Starter Annunciator -->
                </Component>

